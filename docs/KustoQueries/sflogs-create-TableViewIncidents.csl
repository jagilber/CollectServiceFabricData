.create-or-alter function with (docstring = "function to create incidents TableView view for sftable",folder = "sftable") 
    TableViewincidents(T:string) {
        TableView(T)
        | where RelativeUri endswith "incidents"
        // todo fill in fields
        | project Timestamp, 
            PartitionKey, 
            RowKey
        | order by Timestamp asc
    }
